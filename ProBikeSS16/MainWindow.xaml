        <Menu DockPanel.Dock="Top">
            <MenuItem Header="{DynamicResource ResourceKey=File}">
                <MenuItem Header="{DynamicResource ResourceKey=Help}" Click="Handbuch"></MenuItem>
                <Separator></Separator>
                <MenuItem x:Name="ExitButton" Header="{DynamicResource ResourceKey=Exit}" Click="MenuItem_Exit_Click"></MenuItem>
            </MenuItem>
            <MenuItem x:Name="menuItemLanguages" Header="{DynamicResource ResourceKey=Language}">
                <MenuItem x:Name="germanHeader" Tag="de-DE" Header="{DynamicResource ResourceKey=LanguageGerman}" IsCheckable="True" IsChecked="True" Click="LanguageHeader_Click"></MenuItem>
                <MenuItem x:Name="englishHeader" Tag="en-US" Header="{DynamicResource ResourceKey=LanguageEnglish}" IsCheckable="True" IsChecked="False" Click="LanguageHeader_Click"></MenuItem>
            </MenuItem>
            <MenuItem x:Name="Settings" Header="{DynamicResource ResourceKey=Settings}">
                <MenuItem x:Name="ProdCalcSettings" Header="{DynamicResource ResourceKey=ProdCalcSettings}">
                    <MenuItem x:Name="EarlyPartsFirst" Header="{DynamicResource ResourceKey=PartsPrio}" IsCheckable="True" IsChecked="False" Click="ProdCalcSettings_OnClick"></MenuItem>
                    <MenuItem x:Name="BikesFirst" Header="{DynamicResource ResourceKey=BikePrio}" IsCheckable="True" IsChecked="True" Click="ProdCalcSettings_OnClick"></MenuItem>
                </MenuItem>
                <MenuItem x:Name="HighOrLow" Header="{DynamicResource ResourceKey=PartSettings}">
                    <MenuItem x:Name="High" Header="{DynamicResource ResourceKey=HighPrio}" IsCheckable="True" IsChecked="False" Click="HighOrLowSettings_OnClick"></MenuItem>
                    <MenuItem x:Name="Low" Header="{DynamicResource ResourceKey=LowPrio}" IsCheckable="True" IsChecked="True" Click="HighOrLowSettings_OnClick"></MenuItem>
                </MenuItem>
            </MenuItem>
            <MenuItem Header="{DynamicResource ResourceKey=Help}">
                <MenuItem Header="{DynamicResource ResourceKey=Manual}" Click="Handbuch"></MenuItem>
            </MenuItem>
        </Menu>
