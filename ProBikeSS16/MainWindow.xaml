        xmlns:wpfTool="clr-namespace:Xceed.Wpf.Toolkit;assembly=Xceed.Wpf.Toolkit"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="{DynamicResource ResourceKey=Title}" Height="800" Width="1000"
        Background="{DynamicResource WindowBackgroundBrush}">
