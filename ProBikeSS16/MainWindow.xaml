                                <DataGridTemplateColumn Width="80">
                                    <DataGridTemplateColumn.Header>
                                        <TextBlock Text="{DynamicResource ResourceKey=Quantity}"></TextBlock>
                                    </DataGridTemplateColumn.Header>
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBox Text="{Binding quantity}" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>

                                <DataGridTextColumn Width="50" Binding="{Binding article}" IsReadOnly="True" >
                                    <DataGridTextColumn.Header>
                                        <TextBlock Text="{DynamicResource ResourceKey=Article}"/>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>

                                <DataGridTemplateColumn Width="80">
                                    <DataGridTemplateColumn.Header>
                                        <TextBlock Text="{DynamicResource ResourceKey=Penalty}"/>
                                    </DataGridTemplateColumn.Header>
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBox Text="{Binding penalty}" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>

                                <DataGridTemplateColumn Width="*">
                                    <DataGridTemplateColumn.Header>
                                        <TextBlock Text="{DynamicResource ResourceKey=Price}"/>
                                    </DataGridTemplateColumn.Header>
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBox Text="{Binding price}"  />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                            </DataGrid.Columns>
                        </DataGrid>
                        <DataGrid Grid.Column="2" Grid.Row="0" AutoGenerateColumns="False" Name="Orderlist" ItemsSource="{Binding}" CanUserAddRows="False" Margin="10,60,10,10">
                            <DataGrid.Columns>
                                <DataGridTextColumn Width="100" Binding="{Binding article}" IsReadOnly="True" >
                                    <DataGridTextColumn.Header>
                                        <TextBlock Text="{DynamicResource ResourceKey=Article}"/>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                                <DataGridTextColumn Width="100" Binding="{Binding quantity}" IsReadOnly="True" >
                                    <DataGridTextColumn.Header>
                                        <TextBlock Text="{DynamicResource ResourceKey=Quantity}"/>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                                <DataGridTextColumn Width="*" Binding="{Binding modus}" IsReadOnly="True" >
                                    <DataGridTextColumn.Header>
                                        <TextBlock Text="{DynamicResource ResourceKey=Mode}"/>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                            </DataGrid.Columns>
                        </DataGrid>
                        <DataGrid Grid.Column="0" Grid.Row="1" Name="Productionlist" AutoGenerateColumns="False" ItemsSource="{Binding}" CanUserAddRows="False" Margin="10,60,10,10">
                            <DataGrid.Columns>
                                <DataGridTextColumn Width="120" Binding="{Binding Item}" IsReadOnly="True" >
                                    <DataGridTextColumn.Header>
                                        <TextBlock Text="{DynamicResource ResourceKey=Item}"/>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                                <DataGridTextColumn Width="*" Binding="{Binding Amount}" IsReadOnly="True" >
                                    <DataGridTextColumn.Header>
                                        <TextBlock Text="{DynamicResource ResourceKey=Amount}"/>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>
                            </DataGrid.Columns>
                        </DataGrid>
                        <!--<DataGrid Grid.Column="1" Grid.Row="1" Name="Workingtimelist" ItemsSource="{Binding}" Margin="10,60,10,10" CanUserAddRows="False" PreviewTextInput="Workingtimelist_PreviewTextInput"></DataGrid>-->
                        <DataGrid Grid.Column="1" Grid.Row="1" ItemsSource="{Binding}" Name="Workingtimelist" AutoGenerateColumns="False" Margin="10,60,10,10" CanUserAddRows="False" >
                            <DataGrid.Columns>
                                <DataGridTextColumn Width="50" Binding="{Binding station}" IsReadOnly="True" >
                                    <DataGridTextColumn.Header>
                                        <TextBlock Text="{DynamicResource ResourceKey=Station}"/>
                                    </DataGridTextColumn.Header>
                                </DataGridTextColumn>

                                <DataGridTemplateColumn Width="120">
                                    <DataGridTemplateColumn.Header>
                                        <TextBlock Text="{DynamicResource ResourceKey=Shifts}"></TextBlock>
                                    </DataGridTemplateColumn.Header>
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBox Text="{Binding shift}" MaxLength="1" PreviewTextInput="TextBox_PreviewTextInput1to3" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>

                                <DataGridTemplateColumn Width="*">
                                    <DataGridTemplateColumn.Header>
                                        <TextBlock Text="{DynamicResource ResourceKey=Overtime}"></TextBlock>
                                    </DataGridTemplateColumn.Header>
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <TextBox Text="{Binding overtime}" PreviewTextInput="TextBox_PreviewTextInput1to240" />
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>
                            </DataGrid.Columns>
                        </DataGrid>
                        <Button Grid.Column="2" Grid.Row="1" Height="100" Width="200" Content="XML-Export" FontSize="30" Click="ButtonBase_OnClick"></Button>
                    </Grid>
                </TabItem>
            </TabControl>
        </Grid>
    </DockPanel>
</Window>
